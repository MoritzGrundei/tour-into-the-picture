function [padded_image] = padding(image,roomVectrices)
%input:
    [m, n] = size(image);


    dif=0;
    for i=1:length(roomVectrices)
        for k=1:2
            if(-roomVectrices{i}(j)>dif && roomVectrices{i}(j)<0)
                dif = -roomVectrices{i}(j);
            end
        end
    end

    padded_image = zeros(m+2*dif,n+2*dif);
    padded_image((dif+1):(end-dif),(dif+1):(end-dif))

end

